  <div class="wrapper wrapper_padding clearfix">
    <div class="col-4 user_signed_in">
      <h2>Contact info</h2>
        <%= form_for user do |f| %>
        <p>
        <%= f.label :first_name %><br>
        <%= f.text_field :fname %>
        </p>
        <p>
        <%= f.label :last_name %><br>
        <%= f.text_field :lname %>
        </p>
        <p>
        <%= f.label :username %><br>
        <%= f.text_field :username %>
        </p>
        <p>
        <%= f.label :password %><br>
        <%= f.text_field :password %>
        </p> 
        <p>
        <%= f.label :email %><br>
        <%= f.text_field :email %>
        </p>
        <p>
        <%= f.label :phone %><br>
        <%= f.text_field :phone %>
        </p>
    </div>
    <div class="col-4 user_signed_in">
      <h2>Address info</h2>
        <p>
        <%= f.label :website %><br>
        <%= f.text_field :website %>
        </p>
        <p>
        <%= f.label :address %><br>
        <%= f.text_field :address %>
        </p>
        <p>
        <%= f.label :city %><br>
        <%= f.text_field :city %>
        </p>
        <p>
        <%= f.label :state %><br>
        <%= f.text_field :state %>
        </p>
        <p>
        <%= f.label :zip %><br>
        <%= f.text_field :zip %>
        </p>
      <%= f.submit "Sign up" %>
      <% end %>
    </div>
    <div class="col-4 user_signed_in"> 
    </div>
  </div>



