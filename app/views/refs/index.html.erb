<div class="wrapper wrapper_padding clearfix">
<div class="sidebar col-4 user_signed_in"></div>
  <div class="col-2 user_signed_in">
    <h4>Reference Index</h4>
    <hr>
      <p>
     <% @refs.each do |ref| %>
      <strong>name:</strong> <%= link_to ref.name, ref_path(ref) %><br>
      <strong>url:</strong> <%= ref.url %><br>
      <strong>format:</strong> <%= ref.format %><br><br>
     <% end %>
    </p>
  </div>
</div>

